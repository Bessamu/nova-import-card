{
    "name": "sparclex/nova-import-card",
    "description": "A card which lets you batch import resources",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "^5.6",
        "laravel/nova": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Sparclex\\NovaImportCard\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sparclex\\NovaImportCard\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
